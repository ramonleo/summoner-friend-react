{
  "name": "electron-react-node-boilerplate",
  "version": "1.0.0",
  "description": "An Electron application with React and Node",
  "main": "./main.js",
  "homepage": "./",
  "scripts": {
    "start": "concurrently \"npm run react-start\" \"npm run server-start\" \"npm run electron-dev\"",
    "react-build": "react-app-rewired build",
    "react-start": "SET NODE_OPTIONS=--openssl-legacy-provider && react-app-rewired start",
    "server-build": "babel server --out-dir build-server",
    "server-start": "nodemon --watch server --exec babel-node server/server.js",
    "electron-dev": "node scripts/dev.js scripts/start.js",
    "electron-pack": "electron-builder -c.extraMetadata.main='./main.js'",
    "preelectron-pack": "npm run react-build && npm run server-build"
  },
  "author": "tbuyse",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "^7.10.5",
    "axios-mock-adapter": "^1.18.2",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^4.1.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^0.2.12",
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0",
    "gh-pages": "^3.2.3",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "3.0.1",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.19.2",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.1.1",
    "@mui/lab": "^5.0.0-alpha.55",
    "@mui/material": "^5.4.4",
    "app-module-path": "^2.2.0",
    "autosuggest-highlight": "^3.2.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "big-json": "^3.1.0",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.26.0",
    "echarts": "^5.2.2",
    "echarts-for-react": "^3.0.2",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "JSONStream": "^1.3.5",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lcu-connector": "latest",
    "lodash": "^4.17.19",
    "merge": "^2.1.1",
    "moment": "^2.29.1",
    "nodemon": "^1.19.1",
    "notistack": "^2.0.3",
    "piping": "^1.0.0-rc.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-material-ui-form-validator": "^3.0.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "shortid": "^2.2.16",
    "yup": "^0.32.11"
  },
  "build": {
    "appId": "com.electron-react-node",
    "files": [
      "build/**/**/*",
      "build-server/**/**/*",
      "node_modules/**/*",
      "./main.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": []
}
